/*This is my practice array. */

#include <iostream>
int main () {

int const NUM_SIZE;
cin>>NUM_SIZE;

int myArray[NUM_SIZE]={0,1,2,3,4} /*To reference any of the numbers, you must subtract one from its place, as the program labels the first element as element 0. */
for (int i = 0; i<5; i++) {
  cout<< "myArray Value at Element" << i << ":" <<myArray[i] << endl;} //This is for inputing values into the array.
  
return 0;
